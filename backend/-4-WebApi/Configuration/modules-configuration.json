{
  "modules": [
    {
      "key": "role",
      "name": "Cargos",
      "description": "Gerenciar cargos e permissões do sistema",
      "icon": "shield",
      "order": 1,
      "isActive": true,
      "permissions": [
        {
          "key": "canView",
          "name": "Visualizar",
          "description": "Visualizar lista e detalhes de cargos",
          "allowedRoutes": [
            { "method": "GET", "path": "/role/getAll" },
            { "method": "GET", "path": "/role/getPaged" },
            { "method": "GET", "path": "/role/{roleId}" }
          ],
          "order": 1,
          "color": "blue"
        },
        {
          "key": "canCreate",
          "name": "Criar",
          "description": "Criar novos cargos",
          "allowedRoutes": [
            { "method": "POST", "path": "/role/create" }
          ],
          "order": 2,
          "color": "green"
        },
        {
          "key": "canEdit",
          "name": "Editar",
          "description": "Editar cargos existentes",
          "allowedRoutes": [
            { "method": "PUT", "path": "/role/{roleId}" }
          ],
          "order": 3,
          "color": "yellow"
        },
        {
          "key": "canDelete",
          "name": "Excluir",
          "description": "Excluir cargos do sistema",
          "allowedRoutes": [
            { "method": "DELETE", "path": "/role/{roleId}" }
          ],
          "order": 4,
          "color": "red"
        }
      ]
    },
